   "source": [
    "import pandas as pd\n",
    "\n",
    "# Convert 'player_dismissed' to numeric\n",
    "delivery_df['player_dismissed'] = delivery_df['player_dismissed'].fillna(\"0\")  # Fill NaN with \"0\"\n",
    "delivery_df['player_dismissed'] = delivery_df['player_dismissed'].apply(lambda x: x if x == \"0\" else \"1\")  # Standardize values\n",
    "delivery_df['player_dismissed'] = delivery_df['player_dismissed'].astype('int')  # Convert to integer\n",
    "delivery_df['wickets_fallen'] = delivery_df.groupby('match_id')['player_dismissed'].transform('cumsum')  # Use transform instead of cumsum directly\n",
    "delivery_df['wickets'] = 10 - delivery_df['wickets_fallen']  # Subtract fallen wickets from 10\n",
    "print(delivery_df.head())"
   ]
